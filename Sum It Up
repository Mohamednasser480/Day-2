#include<iostream>
#include<vector>
#include<algorithm>
#include<set>
#include<string>
using namespace std;
int main ()
{
	int n,m;
	while(cin>>n>>m&&n){
	vector<int>v(m);
	 set<vector<int>>st;
	for(int i=0;i<m;i++)
		cin>>v[i];
	for(int i=0;i<(1<<m);i++){
		int sum=0;
	     vector<int>v1;
	for(int j=0;j<m;j++)
		if(i&(1<<j)){sum+=v[j]; v1.push_back(v[j]); } 
		if(sum==n)st.insert(v1);
	   }
	cout<<"Sums of "<<n<<":\n";
	if(st.empty())cout<<"NONE\n"; 
	else {
		for(auto it =--st.end();it!=--st.begin();it--){
			for(auto it1=it->begin();it1!=it->end();it1++){
				if(it1++!=--it->end())cout<<*--it1<<"+";
				else cout<<*--it1;
			  }
			cout<<"\n";
			}
	    }
  }
	return 0;
}
